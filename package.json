{
  "version": "2.9.0",
  "name": "fp-ts-remote-data",
  "description": "Remote data type for fp-ts",
  "homepage": "https://github.com/sylcastaing/fp-ts-remote-data",
  "keywords": [
    "fp",
    "fp-ts",
    "remote-data"
  ],
  "author": "Sylvain Castaing @sylcastaing",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sylcastaing/fp-ts-remote-data.git"
  },
  "bugs": "https://github.com/sylcastaing/fp-ts-remote-data/issues",
  "main": "dist/index.js",
  "module": "dist/fp-ts-remote-data.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "start": "dts watch",
    "build": "dts build",
    "test": "dts test",
    "test-watch": "dts test --watch",
    "coverage": "dts test --coverage",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "docs": "docs-ts",
    "version": "dts build && docs-ts",
    "release": "np"
  },
  "peerDependencies": {
    "fp-ts": "^2.11.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "7.0.8",
    "@tsconfig/recommended": "1.0.1",
    "docs-ts": "0.6.10",
    "dts-cli": "1.3.0",
    "fp-ts": "^2.11.5",
    "husky": "7.0.4",
    "np": "7.6.0",
    "pinst": "2.1.6",
    "size-limit": "7.0.8",
    "tslib": "2.3.1",
    "typescript": "4.5.5"
  },
  "size-limit": [
    {
      "path": "dist/fp-ts-remote-data.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/fp-ts-remote-data.esm.js",
      "limit": "10 KB"
    }
  ],
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "endOfLine":"auto"
  },
  "engines": {
    "node": ">=10"
  }
}
